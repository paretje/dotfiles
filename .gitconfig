[user]
	name = Kevin Velghe
[push]
	default = simple
[instaweb]
	httpd = webrick
	browser = xdg-open
[gcrypt]
	participants = 64AD7E10
	signingkey = 64AD7E10
[alias]
	graph = log --graph --decorate --oneline --all
	clean-tags = fetch --prune origin +refs/tags/*:refs/tags/*
[commit]
	gpgsign = true
[diff]
	algorithm = histogram
	colorMoved = true
[merge]
	conflictStyle = diff3
	log = true
[pull]
	ff = only
[filter "lfs"]
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
	required = true
[init]
	defaultBranch = main
